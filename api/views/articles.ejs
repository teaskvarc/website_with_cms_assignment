<!DOCTYPE>

<html>
<head>

    <link href="/static/css/main.css" rel="stylesheet">


</head>
<body>

    <h1>Articles</h1>

    <% for(var i=0; i<articles.length; i++){ %>

    <div class="article-container">

        <h3><%- articles[i].title  %></h3>
        <p><%- articles[i].content %></p>
        <p><%- articles[i].author %></p>
        <p><%- articles[i].published %> </p>

    </div>
    <hr/>
    <% } %>

</body>
</html>